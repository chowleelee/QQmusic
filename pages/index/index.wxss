Page{
  font-family: "微软雅黑";
  font-size: 28rpx;
}

.nav .navCont{
  line-height:80rpx;
  display: flex;
}
.nav .navCont text{
  flex:auto;
  text-align: center;
  align-self: center;
  font-size: 32rpx;
}
.nav .bar{
  width: calc(100%/3);
  height:4rpx;
  background: #31c27c;
  transition:all 1s;
}

.banner{
  width:750rpx;
  height:300rpx;
}
.banner image{
  width:100%;
  height:100%;
}

::-webkit-scrollbar{
  width:0;
  height:0;
  color:transparent;
}
.TJ{
  background:#F4F4F4;
}
.TJ .title{
  display: inline-block;
  font-size: 32rpx;
  margin-bottom: 20rpx;
}
.TJ .TJ_radio, .TJ .TJ_hot{
  padding:20rpx 20rpx;
}
.TJ .TJ_content{
  display: flex;
  justify-content: space-between;
}
.TJ .TJ_content .item{
  width:347rpx;
  background:#fff;
  margin-bottom:20rpx;
  position: relative;
  overflow:hidden;
}
.TJ .TJ_content .item:before{
  content:'';
  position:absolute;
  right:10rpx;
  bottom:85rpx;
  width:48rpx;
  height:48rpx;
  background:url(data:image/png;base64,) no-repeat;
  background-size:contain;
}
.TJ .TJ_content .item image{
  width:347rpx;
  height:347rpx;
}
.TJ .TJ_content .item text{
  display: inline-block;
  width:307rpx;
  padding:0 20rpx 20rpx 20rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.TJ .TJ_content .item .qty{
  display: inline-block;
  font-size: 24rpx;
  position: absolute;
  left:-10rpx;
  bottom:70rpx;
  color:#fff;
}
.TJ .TJ_content .item .qty:before{
  display:inline-block;
  margin-right:10rpx;
  content:'';
  width:20rpx;
  height:20rpx;
  background:url(data:image/png;base64,) no-repeat;
  background-size:contain;
}
.TJ .TJ_hot .TJ_content{
  flex-wrap: wrap;
}

.PHB{
  background:#F4F4F4;
  overflow: hidden;
}
.PHB .item{
  margin:20rpx;
  background:#fff;
  display:flex;
  justify-content: space-between;
}
.PHB .item .img{
  position: relative;
}
.PHB .item .img image{
  width:200rpx;
  height:200rpx;
  vertical-align:top;
}
.PHB .item .img text{
  position: absolute;
  left:10rpx;
  bottom:20rpx;
  color:#fff;
  font-size:24rpx;
}
.PHB .item .img text:before{
  display:inline-block;
  margin-right:10rpx;
  content:'';
  width:20rpx;
  height:20rpx;
  background:url(data:image/png;base64,) no-repeat;
  background-size:contain;
}
.PHB .item .txt{
  width:100%;
  margin-left:30rpx;
}

.PHB .item .txt .title{
  width:400rpx;
  display: inline-block;
  font-size:32rpx;
  line-height: 56rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.PHB .item .txt text+text{
  margin-left:10rpx;
}
.PHB .item .txt .txt_cont{
  width:400rpx;
  display: block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.PHB .item .line{
  align-self: center;
  margin-right:20rpx;
}

.SS{
 background:#F4F4F4;
 overflow: hidden;
}
.SS .search{
  display: flex;
  margin:20rpx;
  font-size: 24rpx;
  justify-content: space-between
}
.SS .search .left{
  flex:auto;
  background: #fff;
  display: flex;
  padding:10rpx 20rpx;
}
.SS .search .left icon{
  align-self: center;
  margin:0 20rpx;
}
.SS .search .left input{
  width:100%;
}
.SS .search .right{
  align-self: center;
  margin-left:30rpx;
}
.SS .hotkey{
  background: #fff;
  padding:40rpx 30rpx;
  overflow: hidden;
}
.SS .hotkey .keywords{
  margin-top:20rpx;
}
.SS .hotkey .keywords text{
  display: inline-block;
  padding:0 20rpx;
  line-height: 60rpx;
  border:1px solid #333;
  border-radius:198rpx;
  margin-bottom:20rpx;
  margin-right:30rpx;
}
.SS .hotkey .keywords .cur{
  border-color:#fc4524;
  color:#fc4524;
}

.SS .result .item{
  background: #fff;
  display: flex;
  border-bottom:1px solid #ccc;
}
.SS .result .item .img{
  width: 44rpx;
  height: 40rpx;
  align-self: center;
  margin:36rpx;
  vertical-align: top;
  background-image: url("../../images/search_sprite.png");
  background-repeat: no-repeat;
  background-size: 22px 30px;
}
.SS .result .item .info{
  width:80%;
  align-self: center;
}
.SS .result .item .info .name{
  width:100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.SS .result .item .info .name text{
  font-size:24rpx;
  color:#808080;
}
.SS .history{
  background:#fff;
}
.SS .history{
  display:flex;
  flex-direction:column;
}
.SS .history .item{
  display:flex;
  justify-content:space-between;
  padding:20rpx 30rpx;
  border-bottom:4rpx solid #f4f4f4;
}
.SS .history .item icon{
  align-self:center;
}
.SS .history .item .cont{
  display:flex;
  flex:auto;
}
.SS .history .item .cont text{
  flex:auto;
  align-self:center;
  margin-left:40rpx;
}
.SS .history .clearAll{
  align-self:center;
  line-height:88rpx;
}


.musicArea{
  position:fixed;
  background:#fff;
  height:70rpx;
  bottom:0rpx;
  width:100%;
  background:#31C27C;
  color:#fff;
  display:flex;

}
.musicArea .songname{
  width:50%;
  line-height:70rpx;
  text-align:center;
}
.musicArea .avatar{
  width:80rpx;
  height:84rpx;
  border-radius:50%;
  position:absolute;
  bottom:0;
  left:calc(50% - 40rpx);
  transition:all 3s infinite;
}
.musicArea image.cur{
  animation:ani 3s infinite linear;
}
@keyframes ani{
  from{
    transform:rotate(0deg);
  }
  to{
    transform:rotate(360deg);
  }
}
.musicArea .playCtr{
  width:50%;
}
.musicArea .playCtr .img{
  /*background-image: url('../../images/sprite_play.png');*/
  background-size: 80rpx 700rpx;
  background-repeat: no-repeat;
  margin-top:18rpx;
  margin-left:calc(50% - 17rpx);
}
.musicArea .playCtr .imgPlay{
  width: 34rpx;
  height: 38rpx;
  background-position: 0 -300rpx;
}
.musicArea .playCtr .imgPause{
  width: 18rpx;
  height: 30rpx;
  background-position: 0 -360rpx;
}